{
  "name": "here_hugo",
  "version": "1.0.0",
  "description": "- [ ] 404 page",
  "main": "index.js",
  "scripts": {
    "build": "bunx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/style.css && hugo --gc --minify && bun pagefind --site public",
    "watch": "bunx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/style.css --watch",
    "dev": "concurrently \"hugo server --navigateToChanged --buildFuture\" \"bun run watch\" \"bun run search:dev\"",
    "search:dev": "bun pagefind --site public --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txndai/here_hugo.git"
  },
  "keywords": [],
  "author": "",
