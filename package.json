  "bugs": {
    "url": "https://github.com/txndai/here_hugo/issues"
  },
  "homepage": "https://github.com/txndai/here_hugo#readme",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "concurrently": "9.1.2",
    "pagefind": "^1.4.0",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {}
}
